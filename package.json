{
  "name": "@caai/di",
  "description": "`@caai/di` is a minimalist TypeScript dependency injection library for Node.js that automatically resolves and injects dependencies into constructors and functions. Designed with zero external dependencies, it powers the Caai framework.",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.17.0"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "dependency injection",
    "di",
    "ioc",
    "inversion of control",
    "typescript",
    "nodejs",
    "minimalist",
    "lightweight",
    "auto inject",
    "constructor injection",
    "function injection",
    "caai",
    "caai-di",
    "clean architecture",
    "zero dependencies",
    "service locator",
    "ts di"
  ],
  "author": "Luiz Leite (luizffl)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luizffl/caai-di.git"
  },
  "bugs": {
    "url": "https://github.com/luizffl/caai-di/issues"
  },
  "devDependencies": {
    "@vitest/expect": "^3.2.4",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
